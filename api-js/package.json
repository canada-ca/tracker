{
  "name": "tracker-api",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run clean && babel ./src --out-dir dist/src --ignore 'src/**/*.spec.js','src/**/*.test.js' && cp ./index.js dist/ && cp ./migrations.js dist/",
    "start": "node ./dist/index.js",
    "dev": "nodemon --exec babel-node index.js",
    "clean": "rm -rf ./dist && mkdir dist",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "lint": "eslint src",
    "prettier": "prettier --write \"**/*.js\"",
    "add-locale": "npx lingui add-locale",
    "extract": "npx lingui extract",
    "compile": "npx lingui compile",
    "lin-clean": "npx lingui extract --clean"
  },
  "author": "nsdeschenes",
  "license": "MIT",
  "dependencies": {
    "@lingui/core": "^2.9.2",
    "apollo-server": "^2.19.0",
    "apollo-server-express": "^2.19.0",
    "arango-tools": "0.0.9",
    "arangojs": "^7.1.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-request-language": "^1.1.15",
    "graphql": "^15.4.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-relay": "^0.6.0",
    "graphql-scalars": "^1.4.1",
    "graphql-subscriptions": "^1.1.0",
    "graphql-validation-complexity": "^0.4.2",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "notifications-node-client": "^5.0.0",
    "psl": "^1.8.0",
    "url-slug": "^2.3.2",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "@lingui/cli": "^2.9.2",
    "@lingui/loader": "^2.9.2",
    "@lingui/macro": "^2.9.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-macros": "^2.8.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-matcher-utils": "^26.6.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1"
  }
}
